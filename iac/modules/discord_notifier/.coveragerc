[run]
source = .
omit = 
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*

[xml]
output = coverage.xml

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[xml]
output = coverage.xml

